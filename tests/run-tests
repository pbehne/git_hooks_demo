#!/usr/bin/env bash

# Make sure we are in same directory as this file
cd "${0%/*}"

# Read test result from file
typeset -i fail=$(cat test_result.txt)

# let's fake failing test for now 
echo "Running tests (bash)"
echo "............................"

if [[ $fail != 0 ]]; then
    echo "Failed!"
else
    echo "Passed!"
fi

exit $fail
